  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "chai": "^3.3.0",
    "eslint": "^1.10.3",
    "ghooks": "^1.0.1",
    "mocha": "^2.3.4"
